{
  "name": "nuxt-sample-project",
  "version": "1.0.0",
  "description": "An example project built using Nuxt",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "cross-env NODE_ENV=development APP_STAGE=development nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "nuxi start",
    "openapi:thecatapi-generate": "openapi-generator-cli generate -g typescript-fetch -i ./api/thecatapi-oas.yaml -o ./api/thecatapi/openapi-generator --additional-properties=withSeparateModelsAndApi=true,useSingleRequestParameter=true,supportsES6=true --model-package=model --api-package=api"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.3.0",
    "@openapitools/openapi-generator-cli": "^2.19.1",
    "@pinia/nuxt": "^0.11.0",
    "@vueuse/core": "^13.1.0",
    "embla-carousel-vue": "^8.6.0",
    "nuxt": "^3.16.2",
    "pinia": "^3.0.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.0.0",
    "@types/node": "^22.14.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "globals": "^16.0.0",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.18.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.4.1",
    "vue-tsc": "^2.2.8"
  }
}
